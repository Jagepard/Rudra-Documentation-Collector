{
    "name": "rudra/docs",
    "description": "Command line tool for generating markdown-formatted class documentation",
    "type": "library",
    "keywords": [
        "rudra",
        "markdown",
        "generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",    
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Docs/issues",    
        "source": "https://github.com/Jagepard/Rudra-Docs"    
    },
    "require": {
        "php": ">=8.3",
        "rudra/cli": "self.version",
        "rudra/container": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Rudra\\Markdown\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    }
}